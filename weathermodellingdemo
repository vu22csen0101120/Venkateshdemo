import math

# Hard-coded variables
a = 1
b = -3
c = 2

# Calculate the discriminant
d = b**2 - 4*a*c

# Calculate the two solutions
if d >= 0:
    sol1 = (-b + math.sqrt(d)) / (2*a)
    sol2 = (-b - math.sqrt(d)) / (2*a)
    print(f"The solutions are {sol1} and {sol2}")
else:
    print("No real number solutions")
